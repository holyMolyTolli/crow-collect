{
  "manifest_version": 3,
  "name": "CrowCollect",
  "version": "1.0",
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  },
  "permissions": ["cookies", "tabs", "activeTab", "storage", "downloads"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/resized/logo16.png",
      "32": "icons/resized/logo32.png",
      "48": "icons/resized/logo48.png",
      "128": "icons/resized/logo128.png",
      "256": "icons/resized/logo256.png"
    }
  },
  "icons": {
    "16": "icons/resized/logo16.png",
    "32": "icons/resized/logo32.png",
    "48": "icons/resized/logo48.png",
    "128": "icons/resized/logo128.png",
    "256": "icons/resized/logo256.png"
  }
}
