{
  "manifest_version": 3,
  "name": "CrowCollect",
  "version": "1.0",
  "permissions": ["activeTab", "cookies", "downloads"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/logo16.png",
      "32": "images/logo32.png",
      "48": "images/logo48.png"
    }
  },
  "icons": {
    "16": "images/logo16.png",
    "32": "images/logo32.png",
    "48": "images/logo48.png"
  },
  "background": {
    "service_worker": "service_worker.js",
    "type": "module"
  }
}
